<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:gravity="center">

    <com.benohayon.meallennium.ui.custom_views.styled_views.StyledTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/home_screen_dont_have_account_frame_sign_up_text_1"
        android:textSize="@dimen/dont_have_account_frame_text_size" />

    <com.benohayon.meallennium.ui.custom_views.styled_views.StyledTextView
        android:id="@+id/signInText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/home_screen_dont_have_account_frame_sign_up_text_2"
        android:textSize="@dimen/dont_have_account_frame_text_size"
        app:fontType="proxima_nova-bold"/>

</LinearLayout>